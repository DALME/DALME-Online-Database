    "crypto-random-string": "^5.0.0",
    "formvuelate": "^3.9.1",
    "js-cookie": "^3.0.5",
    "luxon": "3.4.4",
    "openseadragon": "^4.1.1",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "quasar": "^2.16.4",
    "quasar-ui-qmarkdown-v2": "^2.0.4",
    "ramda": "^0.30.0",
    "string": "^3.3.3",
    "timeago.js": "^4.0.2",
    "vee-validate": "^4.12.8",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2",
    "xstate": "^5.13.0",
    "yup": "^1.4.0"

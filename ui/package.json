    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.2.0",
    "eslint-plugin-vue": "^9.26.0",
    "fast-check": "^3.18.0",
    "npm-check-updates": "^16.14.20",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "vitest": "^2.0.5"

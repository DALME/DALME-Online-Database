{
  "name": "ida.ui",
  "version": "0.0.1",
  "description": "A user interface for the IDA online database.",
  "productName": "IDA",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "quasar build",
    "dev": "quasar dev",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "vitest run --coverage",
    "test:unit:ci": "vitest run",
    "test:unit:ui": "vitest --ui --watch --coverage.enabled=true",
    "test:unit:debug": "vitest --inspect-brk --no-file-parallelism",
    "vsclint": "npx eslint --ext .js,.vue ./"
  },
  "dependencies": {
    "@codemirror/commands": "6.x",
    "@codemirror/language": "6.x",
    "@codemirror/state": "6.x",
    "@codemirror/view": "6.x",
    "@codemirror/search": "6.x",
    "@codemirror/lang-xml": "6.x",
    "@codemirror/theme-one-dark": "6.x",
    "@paralleldrive/cuid2": "^2.2.2",
    "@popperjs/core": "^2.11.8",
    "@quasar/extras": "^1.16.4",
    "@vueuse/components": "^11.1.0",
    "@vueuse/core": "^11.1.0",
    "@xstate/vue": "^3.1.4",
    "CETEIcean": "^1.9.2",
    "axios": "^1.7.1",
    "base64-arraybuffer": "^1.0.2",
    "camel-case": "^5.0.0",
    "camelcase-keys": "^9.1.3",
    "change-case": "^5.4.4",

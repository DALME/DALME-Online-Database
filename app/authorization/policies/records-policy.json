[
  {
    "action": [
      "*"
    ],
    "principal": [
      "group:Administrators"
    ],
    "effect": "allow"
  },
  {
    "action": [
      "list",
      "retrieve",
      "get_set",
      "has_permission",
      "get_manifest",
      "metadata"
    ],
    "principal": [
      "authenticated"
    ],
    "effect": "allow"
  },
  {
    "action": [
      "create"
    ],
    "principal": [
      "group:Users"
    ],
    "effect": "allow"
  },
  {
    "action": [
      "partial_update",
      "change_description",
      "update",
      "bulk_edit"
    ],
    "principal": [
      "authenticated"
    ],
    "effect": "allow",
    "condition_expression": [
      "(user_must_be:owner or has_perm:can_edit)"
    ]
  },
  {
    "action": [
      "destroy",
      "bulk_remove"
    ],
    "principal": [
      "authenticated"
    ],
    "effect": "allow",
    "condition_expression": [
      "(user_must_be:owner or has_perm:can_delete)"
    ]
  }
]
